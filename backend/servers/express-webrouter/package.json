{
  "name": "express-webrouter",
  "main": "app/index.js",
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "6.22.0",
    "concurrently": "^3.1.0",
    "ejs": "^1.0.0",
    "express": "^4.14.0",
    "nodemon": "^1.11.0"
  },
  "scripts": {
		"compile": "babel --presets latest -d app/lib/ app/src/",
		"dev": "concurrently \"npm run dev-start\" \"npm run dev-watch\"",
		"dev-start": "npm run compile && nodemon --watch app/lib --watch app/index.js --exec \"node scripts/manage.js\"",
		"dev-watch": "nodemon --watch app/src --exec \"npm run compile\""
	}
}
